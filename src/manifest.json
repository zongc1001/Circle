{
    "name": "Circle",
    "version": "1.0",
    "description": "Watch vedio with your peer synchronously!",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icons": {
            "16": "./asset/img/icon.png",
            "32": "./asset/img/icon.png",
            "48": "./asset/img/icon.png",
            "128": "./asset/img/icon.png"
        },
        "default_title": "Circle"
    },

    "options_page": "options.html",

    "icons": {
        "16": "./asset/img/icon.png",
        "32": "./asset/img/icon.png",
        "48": "./asset/img/icon.png",
        "128": "./asset/img/icon.png"
    },
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "webNavigation",
        "tabs",
        "http://*.youku.com/*",
        "https://*.youku.com/*",
        "http://*.sohu.com/*",
        "https://*.sohu.com/*",
        "http://*.tudou.com/*",
        "https://*.tudou.com/*",
        "http://*.qq.com/*",
        "https://*.qq.com/*",
        "http://*.iqiyi.com/*",
        "https://*.iqiyi.com/*",
        "http://*.youtube.com/*",
        "https://*.youtube.com/*",
        "http://*.acfun.cn/*",
        "https://*.acfun.cn/*",
        "http://*.bilibili.com/*",
        "https://*.bilibili.com/*",
        "http://*.mgtv.com/*",
        "https://*.mgtv.com/*",
        "http://*.vimeo.com/*",
        "https://*.vimeo.com/*"
    ],
    "background": {
        "scripts": ["./dist/background.js"],
        "persistent": true
    },

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    },
    "web_accessible_resources": [
        "popup.js",
        "dist/background.js",
        "inject.js",
        "options.js",
        "_generated_background_page.html"
    ]

}